# Provider Selection
PRIMARY_PROVIDER=acrcloud
PROVIDER_FALLBACK_ENABLED=true
PROVIDER_FALLBACK_ORDER=shazam

# ACRCloud Configuration
ACR_ACCESS_KEY=your_access_key_here
ACR_ACCESS_SECRET=your_access_secret_here
ACR_HOST=identify-eu-west-1.acrcloud.com
ACR_TIMEOUT=10

# Shazam Configuration
SHAZAM_ENABLED=true
SHAZAM_TIMEOUT=10

# Spotify Configuration
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SPOTIFY_TIMEOUT=10

# Track Identification Settings
SEGMENT_LENGTH=60
MIN_CONFIDENCE=0.0
TIME_THRESHOLD=60
MAX_DUPLICATES=2

# Output Settings
OUTPUT_FORMAT=json
OUTPUT_DIRECTORY=output

# Application Settings
VERBOSE=false
DEBUG=false
MAX_REQUESTS_PER_MINUTE=60
RATE_LIMIT_ENABLED=true

# Cache Settings
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_MAX_SIZE=1000
