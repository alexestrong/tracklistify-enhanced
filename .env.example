# ACRCloud Configuration
ACR_ACCESS_KEY=your_access_key_here
ACR_ACCESS_SECRET=your_access_secret_here
ACR_HOST=identify-eu-west-1.acrcloud.com
ACR_TIMEOUT=10

# Shazam Configuration
SHAZAM_API_KEY=your_shazam_api_key_here
SHAZAM_TIMEOUT=10

# Spotify Configuration
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SPOTIFY_TIMEOUT=10

# Track Identification Settings
SEGMENT_LENGTH=30
MIN_CONFIDENCE=70
TIME_THRESHOLD=60
MAX_DUPLICATES=2

# Audio Processing Settings
SAMPLE_RATE=44100
WINDOW_SIZE=2048
HOP_LENGTH=512
N_MELS=128
PRE_EMPHASIS=0.97

# Output Settings
OUTPUT_FORMAT=json
OUTPUT_DIR=tracklists

# Application Settings
VERBOSE=false
MAX_REQUESTS_PER_MINUTE=60
RATE_LIMIT_ENABLED=true
CACHE_ENABLED=true
CACHE_DIR=.cache
LOG_LEVEL=INFO
LOG_FILE=tracklistify.log
